{
    "version": 2,
    "builds": [
        {
            "src": "app/**",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "monthly_challenges/wsgi.py "
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
